<odoo>
	<data>
		<record id="product_garment_brand_tree_view" model="ir.ui.view">
			<field name="name">product.garment.brand.tree.view</field>
			<field name="model">product.garment.brand</field>
			<field name="arch" type="xml">
				<tree string="Product Brand" editable="top">
					<field name="name"/>
				</tree>
			</field>
		</record>
		<record id="product_garment_brand_form_view" model="ir.ui.view">
			<field name="name">product.garment.brand.form.view</field>
			<field name="model">product.garment.brand</field>
			<field name="arch" type="xml">
				<form string="Product Brand">
					<group>
						<group>
							<field name="name"/>
						</group>
					</group>
				</form>
			</field>
		</record>
		<record id="product_garment_brand_action" model="ir.actions.act_window">
			<field name="name">Brands</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">product.garment.brand</field>
			<field name="view_mode">tree</field>
			<field name="view_type">form</field>
			<field name="arch" type="xml">
				<form string="Product Brand">
					<group>
						<group>
							<field name="name"/>
						</group>
					</group>
				</form>
			</field>
		</record>
		<menuitem id="product_garment_brand_menu" action="product_garment_brand_action" name="Product Brands" parent="sale.prod_config_main" groups="sales_team.group_sale_manager" />
	</data>
</odoo>