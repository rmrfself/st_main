          <!-- Modal -->
          <!-- Modal -->
          <div class="modal fade" id="gmt-upload-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">x</span>
                  </button>
                  <h4 class="modal-title" id="myModalLabel">Garment Upload</h4>
                </div>
                <div class="modal-body" id="gmt-upload">
                  <div class='content'>
                    <div class="row">
                      <form class="form-horizontal">
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class='col-sm-3' for="exampleInputName2">Category</label>
                            <div class="col-sm-9">
                              <select id='gmt-category' class="form-control form-control-lg">
                              </select>
                            </div>
                          </div>
                          <div class="form-group row">
                            <label class='col-sm-3' for="exampleInputName2">Style</label>
                            <div class="col-sm-9">
                              <select id='gmt-style' class="form-control form-control-lg">
                              </select>
                            </div>
                          </div>
                          <div class="form-group row">
                            <label class='col-sm-3' for="gmt-brand">Brand</label>
                            <div class="col-sm-9">
                              <input type="text" class="form-control" id="gmt-brand" placeholder="brand" />
                            </div>
                          </div>
                          <div class="form-group row color-edit">
                            <a href="#" id='gmt-color-picker' class='glyphicon glyphicon-plus'>
                              <span>add</span>
                            </a>
                            <label class='col-sm-3' for="gmt-brand">Color</label>
                            <div class="col-sm-12 color-blocks" id='gmt-colors'></div>
                          </div>
                          <div class="form-group row gmt-image-upload">
                            <label class='col-sm-5' for="gmt-brand">Image Upload</label>
                            <div class="col-sm-12 image-blocks">
                              <table class='table table-bordered'>
                                <tr>
                                  <td>
                                    <a href="#" class='glyphicon glyphicon-open upload-link'></a>
                                    <span>top</span>
                                  </td>
                                  <td>
                                    <a href="#" class='glyphicon glyphicon-open upload-link'></a>
                                    <span>left</span>
                                  </td>
                                  <td>
                                    <a href="#" class='glyphicon glyphicon-open upload-link'></a>
                                    <span>front</span>
                                  </td>
                                </tr>
                                <tr>
                                  <td>
                                    <a href="#" class='glyphicon glyphicon-open upload-link'></a>
                                    <span>bottom</span>
                                  </td>
                                  <td>
                                    <a href="#" class='glyphicon glyphicon-open upload-link'></a>
                                    <span>right</span>
                                  </td>
                                  <td>
                                    <a href="#" class='glyphicon glyphicon-open upload-link'></a>
                                    <span>back</span>
                                  </td>
                                </tr>
                              </table>
                            </div>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class='col-sm-2' for="exampleInputName2">Style</label>
                            <div class="col-sm-9">
                              <select id='gmt-style' class="form-control form-control-lg">
                                <option>US</option>
                                <option>Urp</option>
                              </select>
                            </div>
                            <div class="col-sm-12 size-list">
                              <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox1" value="option1" /> S
                              </label>
                              <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox1" value="option1" /> M
                              </label>
                              <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox1" value="option1" /> L
                              </label>
                              <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox1" value="option1" /> XS
                              </label>
                              </div>
                              <div class="col-sm-12 size-list" style='margin-top:0;'>
                              <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox1" value="option1" /> XL
                              </label>
                              <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox1" value="option1" /> 2XL
                              </label>
                              <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox1" value="option1" /> 3XL
                              </label>
                              <label class="checkbox-inline">
                                <input type="checkbox" id="inlineCheckbox1" value="option1" /> 4XL
                              </label>
                            </div>
                          </div>
                          <div class="form-group row">
                            <label class='col-sm-3' for="gmt-brand">Description</label>
                            <div class="col-sm-12">
                              <textarea class="form-control" rows="7" id="comment"></textarea>
                            </div>
                          </div>
                          <div class="form-group row submits">
                          <div class="col-md-12 text-center"> 
