          <!-- Modal -->
          <!-- Modal -->
          <div class="modal fade" id="gmt-upload-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">x</span>
                  </button>
                  <h4 class="modal-title" id="myModalLabel">Garment Upload</h4>
                </div>
                <div class="modal-body" id="gmt-upload">
                  <div class='content'>
                    <div class="row">
                      <form class="form-horizontal">
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class='col-sm-3' for="exampleInputName2">Category</label>
                            <div class="col-sm-9">
                              <select id='gmt-category' class="form-control form-control-lg">
                              </select>
                            </div>
                          </div>
                          <div class="form-group row">
                            <label class='col-sm-3' for="exampleInputName2">Style</label>
                            <div class="col-sm-9">
                              <select id='gmt-style' class="form-control form-control-lg">
                              </select>
                            </div>
                          </div>
                          <div class="form-group row">
                            <label class='col-sm-3' for="gmt-brand">Brand</label>
                            <div class="col-sm-9">
                              <input type="text" class="form-control" id="gmt-brand" placeholder="brand" />
                            </div>
                          </div>
                          <div class="form-group row color-edit">
                            <a href="#" id='gmt-color-picker' class='glyphicon glyphicon-plus'>
                              <span>add</span>
                            </a>
                            <label class='col-sm-3' for="gmt-brand">Color</label>
                            <div class="col-sm-12 color-blocks" id='gmt-colors'></div>
                          </div>
                          <div class="form-group row gmt-image-upload">
                            <label class='col-sm-5' for="gmt-brand">Image Upload</label>
                            <div class="col-sm-12 image-blocks">
                              <table class='table table-bordered'>
                                <tr>
                                  <td>
                                    <input type="file" id="gmt-img-top" style="display: none" />
                                    <a href="#" id='gmt-link-top' class='glyphicon glyphicon-open upload-link'></a>
                                    <span>top</span>
                                  </td>
                                  <td>
                                    <input type="file" id="gmt-img-left" style="display: none" />
                                    <a href="#" id='gmt-link-left' class='glyphicon glyphicon-open upload-link'></a>
                                    <span>left</span>
                                  </td>
                                  <td>
                                    <input type="file" id="gmt-img-front" style="display: none" />
                                    <a href="#" id='gmt-link-front' class='glyphicon glyphicon-open upload-link'></a>
                                    <span>front</span>
                                  </td>
                                </tr>
                                <tr>
                                  <td>
                                    <input type="file" id="gmt-img-bottom" style="display: none" />
                                    <a href="#" id='gmt-link-bottom' class='glyphicon glyphicon-open upload-link'></a>
                                    <span>bottom</span>
                                  </td>
                                  <td>
                                    <input type="file" id="gmt-img-right" style="display: none" />
                                    <a href="#" id='gmt-link-right' class='glyphicon glyphicon-open upload-link'></a>
                                    <span>right</span>
                                  </td>
                                  <td>
                                    <input type="file" id="gmt-img-back" style="display: none" />
                                    <a href="#" id='gmt-link-back' class='glyphicon glyphicon-open upload-link'></a>
                                    <span>back</span>
                                  </td>
                                </tr>
                              </table>
                            </div>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class='col-sm-5' for="exampleInputName2">Size Template</label>
                            <div class="col-sm-7">
                              <select id='gmt-size-tpl' class="form-control form-control-lg">
                              </select>
                            </div>
                            <div class="col-sm-12 size-list" id='size-attrs'></div>
                          </div>
                          <div class="form-group row">
                            <label class='col-sm-3' for="gmt-brand">Description</label>
                            <div class="col-sm-12">
                              <textarea class="form-control" rows="7" id="comment"></textarea>
                            </div>
                          </div>
                          <div class="form-group row submits">
                            <div class="col-md-12 text-center">
                              <button type="button" id='gmt-cancel-btn' class="btn btn-primary">Cancel</button>
                              <button type="button" class="btn btn-success">Save</button>
                            </div>
                          </div>
                        </div>
                      </form>
                    </div>
