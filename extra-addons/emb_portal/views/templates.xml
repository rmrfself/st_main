    <template id='footer_copyright_emb' inherit_id='website.layout' name='Emb Footer Copyright'>
      <xpath expr='//footer' position='replace'>
        <div class='container mt16 mb8'></div>
      </xpath>
    </template>
    <template id='assets_frontend_emb_portal' inherit_id='web.assets_frontend' name='Emb Portal Frontend Assets'>
      <xpath expr='//link[last()]' position='after'>
        <link rel='stylesheet' type='text/less' href='/emb_portal/static/src/css/spectrum.css'/>
        <link rel='stylesheet' type='text/less' href='/web/static/lib/bootstrap/less/mixins/grid.less'/>
        <link rel='stylesheet' type='text/less' href='/emb_portal/static/src/css/main.less'/>
      </xpath>
      <xpath expr='//script[last()]' position='after'>
        <script type='text/javascript' src='/emb_portal/static/src/js/spectrum.js'></script>
        <script type='text/javascript' src='/emb_portal/static/src/js/garment_upload.js'></script>
        <script type='text/javascript' src='/emb_portal/static/src/js/box_designer.js'></script>
      </xpath>
    </template>
