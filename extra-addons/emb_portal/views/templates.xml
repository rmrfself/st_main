    <template id='cart_history' name='Emb Portal Cart History'>
      <t t-call='website.layout'>
        <div class='container emb_portal'>
          <h2>History List</h2>
          <div>
            <h3>Order Name</h3>
            <input type='checkbox' />
          </div>
        </div>
      </t>
    </template>  
    <template id='cart_list' name='Cart List'>
      <t t-call='website.layout'>
        <div id='app_container'>
          <div>ordername</div>
          <div>
            <t>
              {order.status}
            </t>
          </div>
        </div>
      </t>
    </template>
  </data>
</odoo>